{
  "name": "twilio_account_switcher",
  "description": "Switch Twilio account on your console.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_title": "twilio_account_switcher",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://jp.twilio.com/console/project/subaccounts*"
      ],
      "js": [
        "nanojs.js",
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "cookies",
    "https://*.twilio.com/*"
  ]
}
